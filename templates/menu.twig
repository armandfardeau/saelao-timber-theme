{% if items %}
    <div class="navbar-links">
        {% for item in items %}
            <div class="navbar-link {{ item.classes | join(' ') }}">
                <a href="{{ item.get_link }}">{{ item.title }}</a>
                {% include "menu.twig" with {'items': item.get_children} %}
            </div>
        {% endfor %}
    </div>
{% endif %}